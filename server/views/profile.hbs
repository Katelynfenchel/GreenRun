<header class="user-header">
		<div class="header-container">
			<div class="row">
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-10">
						<div class="circle"></div>
						<h1 class="title">{{username}}</h1>
				</div>
				<div class="col-lg-8 col-md-8 col-sm-8 col-sx-6">
					<nav>
						<div class="user-nav">
							<a href="/home">Settings</a>
							<a href="/user/logout">Sign Out</a>
							
						</div>
					</nav>
				</div>
			</div>
		</div>
</header>	
</section class="user-mountains">		
			<div class="row">
				<div class="col-lg-8 col-md-8 col-sm-8 col-sx-6">
					{{#each favoriteMountainArray}}
					
						<div class="mountainName">
						<p class="favMount">{{this.name}} 21<span>&deg;F</span><img class="weather-icon" src="../images/SVG/Sun.svg"/></p>
						
										{{!-- <canvas id="icon1" width="128" height="128"></canvas> --}}		
						</div>
					{{/each}}
				</div>
			</div>
</section>

			



